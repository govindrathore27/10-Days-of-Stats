   "source": [
    "# Enter your code here. Read input from STDIN. Print output to STDOUT\n",
    "num,den = list(map(int,input().split(\" \")))\n",
    "n = int(input())\n",
    "p = num/den\n",
    "q = 1 - p\n",
    "def geometric(n,p,q):\n",
    "    return q**(n-1)*p\n",
    "print(round(sum(list(map(lambda x:geometric(5-x,p,q),range(n)))),3))\n"
   ]
