    "# Enter your code here. Read input from STDIN. Print output to STDOUT\n",
    "n = int(input().strip())\n",
    "x = [int(x) for x in input().split(\" \")]\n",
    "u = sum(x)/len(x)\n",
    "variance = 0\n",
    "for i in x:\n",
    "    variance = variance + (i - u)**2\n",
    "variance = variance/len(x)\n",
    "sd = variance**(0.5)\n",
    "print(round(sd,1))\n"
   ]
